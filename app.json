{
  "expo": {
    "name": "Online Doctor",
    "slug": "online-doctor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo1024.png",
    "splash": {
      "image": "./assets/splash-img.png",
      "resizeMode": "contain",
      "backgroundColor": "#3358b0"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.barack.my-doctor"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.alfred.mydoctor"
    },
    "web": {
      "favicon": "./assets/logo1024.png"
    },
    "description": "My Doctor is a mobile application that was developed and presented by\n      Alfred Bisimwa, a ULK Kigali student in the\n          the academic Year 2021-2022, to help the population keep in touch with\n          their doctors.",
    "githubUrl": "https://github.com/barackm/Doctor-App"
  }
}
